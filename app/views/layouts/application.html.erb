<!DOCTYPE html>
<html>
<head>
  <title>SportyChickApparel</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <link href=' http://fonts.googleapis.com/css?family=Roboto+Condensed' rel='stylesheet' type='text/css'>
  <link href=' http://fonts.googleapis.com/css?family=Dosis' rel='stylesheet' type='text/css'>
  <link href=' http://fonts.googleapis.com/css?family=Wire+One' rel='stylesheet' type='text/css'>
 <link rel="icon" type="image/ico" href="faviconico2.png"/>
</head>
<body>
<!-- facebook code -->
<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
//  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=109498679086250";
  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=1443433645884237";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<!-- end facebook code -->

<div id="main-wrapper">
  <div class="container-fluid header-container">
    <%= render 'layouts/header' %>
  </div>

  <div class="container-fluid navbar-container">
    <div class="row-fluid">
      <div class="span12">
        <%= render 'layouts/navbar' %>
      </div>
    </div> <!-- end main navbar row -->
  </div> <!-- end container-fluid -->

  <!-- <div class="container"> -->
    <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>
    <%= yield %>
  <!-- </div>-->
  
  <div id="push"></div>
</div> <!-- end main-wrapper -->

<%= render 'layouts/footer' %>


</body>
</html>
