<% unless @accessories.nil? %>
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Accessories
      <span class="caret"></span>
    </a>
    <ul class="dropdown-menu">
      <% @acccessories.each do |accessory| %>
        <li><%= link_to accessory.to_s.pluralize, :controller => 'shopping', :action => 'accessories',
              :accessory_id => accessory.id %></li>
      <% end %>
    </ul>
  </li>
<% end %>

