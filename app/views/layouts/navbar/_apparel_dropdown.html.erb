<% unless @apparels.nil? %>
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Apparel
      <span class="caret"></span>
    </a>
    <ul class="dropdown-menu">
      <% @apparels.each do |apparel| %>
        <li><%= link_to apparel.to_s.pluralize, :controller => 'shopping', :action => 'apparel',
              :apparel_id => apparel.id %></li>
      <% end %>
    </ul>
  </li>
<% end %>
